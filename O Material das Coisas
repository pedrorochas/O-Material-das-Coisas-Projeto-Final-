function setup() {
  createCanvas(600, 500);
}//Espaço
function draw(){
  
            if(keyIsDown(ESCAPE)){
              clear();
              tela=1;
              inte=1;
              reinte=0;
              mov=1;
              x1=20;
              y1=390;
              ferroinv=0;
              madeirainv=0;
              tecidoinv=0;
              marteloinv=0;
              tesourainv=0;
              musimenufreq=0;
              musifase1.stop();
              musivila1.stop();
              musipenhasco.stop();
              musimenu.stop();
  }
            else{}
  
  
if(tela==1){
  clear();
  image(menu, 0, 0);
  image(persomenu, 430, 230);
  image(tesomenu, 100, 110);
  image(bicimenu, 30, 200);
  image(ferromenu, 450, 20);
  
  if(musimenufreq==0){
    musimenu.loop(0, 1, 1);
    musimenufreq=1;
  }
  else{}
  
  
  if(mouseX>214&&mouseX<360&&mouseY>177&&mouseY<237){
    strokeWeight(1);
    stroke(0);
    fill(20, 120, 250, 170)
    rect(215, 178, 145, 59, 10 )
  }
  else{
    strokeWeight(1);
    stroke(0);
    noFill()
    rect(215, 178, 145, 59, 10 )
  }
  if(mouseX>174&&mouseX<410&&mouseY>271&&mouseY<341){
    strokeWeight(1);
    fill(20, 120, 250, 170)
    rect(175, 272, 235, 70, 10 )
  }
  else{
    strokeWeight(1);
    noFill()
    rect(175, 272, 235, 70, 10 )
  }
  if(mouseX>196&&mouseX<388&&mouseY>374&&mouseY<436){
    strokeWeight(1);
    fill(20, 120, 250, 170)
    rect(197, 374, 192, 63, 10)
  }
  else{
    strokeWeight(1);
    noFill()
    rect(197, 374, 192, 63, 10)
  }
  
if(mouseIsPressed){
  if(mouseX>214&&mouseX<360&&mouseY>177&&mouseY<237){
    tela=2;
  }
  else{}
}
  else{}
if(mouseIsPressed){
  if(mouseX>174&&mouseX<410&&mouseY>271&&mouseY<341){
    tela=3;
  }
  else{}
}
  else{}
if(mouseIsPressed){
  if(mouseX>196&&mouseX<388&&mouseY>374&&mouseY<436){
    tela=4;
  }
  else{}
}
  else{}
  
    noStroke()
    textSize(50);
    fill(40, 40, 100, 255);
    text('O Material das Coisas', 50, 100);
  
    textSize(45);
    fill(40, 40, 40, 255);
    text('Jogar', 230, 220);
  
    textSize(45);
    fill(40, 40, 40, 255);
    text('Instruções', 190, 320);
  
    textSize(45);
    fill(40, 40, 40, 255);
    text('Créditos', 210, 420);
  
    fill(20, 20, 20, 70);
    ellipse(486, 403, 84, 7);
  
    fill(20, 20, 20, 70);
    ellipse(114, 367, 152, 7);
    } //Menu
    else{}

if(tela==2){
    clear();
    image(menu, 0, 0);
    image(iconefase1, 30, 130);
  
if(mouseX>39&&mouseX<136&&mouseY>420&&mouseY<470){
  stroke(0)
  fill(20, 120, 250, 170)
  rect(40, 415, 97, 50, 10 )
}
  else{
    stroke(0)
    noFill()
    rect(40, 415, 97, 50, 10 )
  }
  
if(mouseIsPressed){
  if(mouseX>39&&mouseX<136&&mouseY>420&&mouseY<470){
    tela=1;
  }
  else{}
}
  else{}
  
if(mouseX>30&&mouseX<170&&mouseY>130&&mouseY<270){
  stroke(0)
  fill(240, 240, 140, 175)
  rect(30, 130, 140, 140)
}
  else{
    stroke(0)
    noFill()
    rect(30, 130, 140, 140)
  }
  
if(mouseX>230&&mouseX<370&&mouseY>130&&mouseY<270){
  stroke(0)
  fill(240, 240, 140, 175)
  rect(230, 130, 140, 140)
}
  else{
    stroke(0)
    noFill()
    rect(230, 130, 140, 140)
  }
  
if(mouseX>430&&mouseX<570&&mouseY>130&&mouseY<270){
  stroke(0)
  fill(240, 240, 140, 175)
  rect(430, 130, 140, 140)
}
  else{
    stroke(0)
    noFill()
    rect(430, 130, 140, 140)
  }
  
if(mouseIsPressed){
  if(mouseX>30&&mouseX<170&&mouseY>130&&mouseY<270){
    musimenu.stop();
    musiintro1.loop(0, 1, 0.7);
    tela=1000;
  }
  else{}
}
  else{}
  
    noStroke()
    textSize(30)
    fill(0, 0, 0, 255);
    text('Voltar', 50, 450);
  
    noFill()
    rect(30, 130, 140, 140)
  
    fill(20, 20, 20, 120);
    rect(230, 130, 140, 140)
  
    fill(20, 20, 20, 120);
    rect(430, 130, 140, 140)
  
    textSize(50);  
    fill(40, 40, 100, 255);
    text('Fases', 225, 80);
  
    textSize(30);
    fill(0, 0, 0, 255);
    text('Fase 1', 57, 305);
  
    textSize(30);
    fill(0, 0, 0, 255);
    text('Fase 2', 257, 305);
  
    textSize(30);
    fill(0, 0, 0, 255);
    text('Fase 3', 457, 305);
  
    textSize(20);
    fill(100, 180, 250, 255);
    text('Em breve...', 250, 210);
    text('Em breve...', 450, 210);
  
    } //Jogar
    else{}
  
if(tela==3){
  clear();
  image(menu, 0, 0);
  
  if(mouseX>39&&mouseX<136&&mouseY>420&&mouseY<470){
  stroke(0)
  fill(20, 120, 250, 170)
  rect(40, 415, 97, 50, 10 )
}
  else{
    stroke(0)
    noFill()
    rect(40, 415, 97, 50, 10 )
  }
  
if(mouseIsPressed){
  if(mouseX>39&&mouseX<136&&mouseY>420&&mouseY<470){
    tela=1;
  }
  else{}
}
  else{}
  
  noStroke()
  textSize(30)
  fill(0, 0, 0, 255);
  text('Voltar', 50, 450);
  
  textSize(50);
  fill(40, 40, 100, 255);
  text('Instruções...', 100, 100)
  
  textSize(20);  
  fill(0, 0, 0, 255);
  text('O Material das Coisas é um jogo desenvolvido', 50, 200);
  text('para o 2° ano do Ensino Fundamental 1, na', 50, 225);
  text('disciplina de ciências.', 50, 250);
  text('O jogo aborda a habilidade EF02CI02: O uso de', 50, 300)
  text('diferentes materiais para a construção de', 50, 325);
  text('objetos de uso cotidiano, tendo em vista as', 50, 350)
  text('propriedades desses materiais.', 50, 375)

  
  
  } //Instruções
  else{}
  
if(tela==4){
  clear();
  image(menu, 0, 0);
  image(pedro, 60, 120);
  image(orientador, 60, 300);
  
if(mouseX>439&&mouseX<555&&mouseY>15&&mouseY<62){
  stroke(0)
  fill(20, 120, 250, 170)
  rect(440, 15, 115, 48, 10 )
}
  else{
    stroke(0)
    noFill()
    rect(440, 15, 115, 48, 10 )
  }
  
if(mouseIsPressed){
  if(mouseX>439&&mouseX<555&&mouseY>15&&mouseY<62){
    tela=1;
  }
  else{}
}
  else{}
  
  noStroke()
  textSize(50);  
  fill(40, 40, 100, 255);
  text('Créditos...', 150, 100);
  
  textSize(20)
  fill(0, 0, 0, 255);
  text('Programador: Pedro Rocha Di Cavalcanti', 180, 250);
  
  textSize(20)
  fill(0, 0, 0, 255);
  text('Orientador: Nenhum...', 215, 440);
  
  textSize(30)
  fill(0, 0, 0, 255);
  text('Voltar', 460, 50);
  
  } //Créditos
  else{}
  
if(tela==11){
  clear();
  image(fundofase1, 0, 0);
  
  

if(x1>=420&&x1<=500&&y1>=200&&y1<=350){
  strokeWeight(2);
  stroke(0, 120, 250, 250);
  fill(240, 220, 70, 190);
  ellipse(480, 347, 50, 15);
}
  else{
  strokeWeight(3);
  stroke(50, 110, 250, 250);
  fill(20, 20, 20, 60);
  ellipse(480, 347, 50, 15); 
  }

  
if(x1>595){
  tela=12;
  x1=-16;
}
  else{}

  image(placasetadir, 450, 260);
  
if(mov==1){
  image(perdirpar, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+68, 35, 5);
}
  else{}
if(mov==2){
  image(perdirand, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+68, 35, 5);
}
  else{}
if(mov==3){
  image(peresqpar, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+68, 35, 5);
}
  else{}
if(mov==4){
  image(peresqand, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+68, 35, 5);
}
  else{}
if(mov==5){
  image(perbaipar, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+64, 35, 5);
}
  else{}
if(mov==6){
  image(perbaiand, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+64, 35, 5);
}
  else{}
if(mov==7){
  image(percimpar, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+70, 35, 5);
}
  else{}
if(mov==8){
  image(percimand, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+70, 35, 5);
}
  else{}
if(mov==12){
  image(perdirand2, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+68, 35, 5);
}
  else{}
if(mov==14){
  image(peresqand2, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+68, 35, 5);
}
  else{}
if(mov==16){
  image(perbaiand2, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+64, 35, 5);
}
  else{}
if(mov==18){
  image(percimand2, x1, y1);
  noStroke()
  fill(20, 20, 20, 70);
  ellipse(x1+25, y1+70, 35, 5);
}
  else{}
  
if(keyIsDown(LEFT_ARROW)){
if(x1>10){
  if(tempo<=1.5){
    mov=4;
    x1=x1-2
    tempo = tempo + 0.1;
  }
  else{}
  if(tempo>1.5&&tempo<3){
    mov=14;
    x1 = x1-2;
    tempo = tempo + 0.1;
  }
  else{}
  if(tempo>=3){
    tempo = 0;
  }
  else{}
  }
  else{}
}
  else{
    if(mov==4 || mov==14){
      tempo=0;
      mov=3;
    }
    else{}
  }
  
if(keyIsDown(RIGHT_ARROW)){
  if(tempo<=1.5){
  mov=2;
  x1=x1+2;
  tempo = tempo + 0.1;
  }
  else{}
  if(tempo>1.5&&tempo<3){
      mov=12;
      x1=x1+2
      tempo = tempo + 0.1;
  }
  else{}
  if(tempo>=3){
    tempo=0;
  }
  else{}
}
  else{
    if(mov==2 || mov==12){
      tempo=0;
      mov=1;
    }
    else{}
  }
  
if(keyIsDown(UP_ARROW)){
if(y1>300){
  if(tempo<=1.5){
  mov=8;
  y1=y1-2
  tempo = tempo + 0.1;  
  }  
  else{}
  if(tempo>1.5&&tempo<3){
    mov=18
    y1=y1-2
    tempo=tempo+0.1;
  }
  else{}
  if(tempo>=3){
    tempo=0;
  }
  else{}
}
  else{}
}
  else{
    if(mov==8 || mov==18){
      tempo=0;
      mov=7;
    }
    else{}
  }
  
if(keyIsDown(DOWN_ARROW)){
if(y1<400){
  if(tempo<=1.5){
  mov=6;
  y1=y1+2
  tempo = tempo + 0.1;
  }
  else{}
  if(tempo>1.5&&tempo<3){
  mov=16;
  y1=y1+2
  tempo = tempo + 0.1;    
  }
  else{}
  if(tempo>=3){
    tempo=0;
  }
  else{}
}
  else{}
}
  else{
    if(mov==6 || mov==16){
    tempo = 0;
    mov=5;
  }
    else{}
}
  
if(keyIsDown(90)){
  inte=2;
}
  else{}
  
if(keyIsDown(88)){
  inte=1;
}
  else{}

if(inte==2){
  if(x1>=420&&x1<=500&&y1>=200&&y1<=350){
      
      strokeWeight(5);
      stroke(240, 220, 70, 250)
      fill(50, 110, 250, 180)
      rect(70, 50, 400, 200);
      
      textSize(20);
      noStroke();
      fill(0, 0, 0, 250);
      text('A Cidade Tranquila fica para lá!', 80, 90);
      text('Continue caminhando para a direita!', 80, 130);
      text('Se você for para a esquerda, chegará na', 80, 190);
      text('Floresta do Sossego... tome cuidado!', 80, 230);
      
  }
  else{}
}
  else{}
  
  
} //Fase1 Estrada da Placa
  else{}
  
if(tela==12){
  clear();
  image(fundofase1, 0, 0);
  
  noStroke();
  fill(20, 20, 20, 120);
  ellipse(315, 355, 380, 10);
  
  
if(x1>=400&&x1<=490&&y1>=280&&y1<=310){
