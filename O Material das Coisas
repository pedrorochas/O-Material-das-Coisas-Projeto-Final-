  ellipse(243, 365, 60, 10);
}
  else{
  strokeWeight(3);
  stroke(50, 110, 250, 250);
  fill(20, 20, 20, 60);
  ellipse(243, 365, 60, 10); 
}

  
  image(velhosus, 430, 240);
  image(casasus, 120, 170);
  
  
  
if(x1<-40){
  x1=566
  tela=11;
}
  else{}
if(x1>595){
  x1=-16;
  tela=13;
  musifase1.stop();
  musivila1.loop(0, 1, 0.25);
}
  else{}
  
if(mov==1){
